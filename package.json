    "format": "prettier --write .",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "build": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug && cd ..",
    "sha": "cd android && ./gradlew signingReport && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "launch-ios": "react-native run-ios --simulator \"iPhone 15 Pro\"",
    "clean": "cd android && ./gradlew clean && cd ..",
    "releaseBundle": "cd android && ./gradlew bundleRelease && cd ..",
    "both": "npx react-native-clean-project --remove-iOS-build --remove-iOS-pods",
    "releaseKeystore": "cd android && keytool -genkey -v -keystore your_key_name.keystore -alias your_key_alias -keyalg RSA -keysize 2048 -validity 10000",
    "releaseApk": "cd android && ./gradlew assembleRelease && cd .."
  },
